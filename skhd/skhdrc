shift + cmd - return : kitty ~/

cmd - up : yabai -m window --focus north
cmd - right : yabai -m window --focus east
cmd - down : yabai -m window --focus south
cmd - left : yabai -m window --focus west

cmd + shift - up : yabai -m window --swap north
cmd + shift - right : yabai -m window --swap east
cmd + shift - down : yabai -m window --swap south
cmd + shift - left : yabai -m window --swap west

cmd + alt - up : yabai -m window --resize top:0:-50; \
                  yabai -m window --resize bottom:0:-50
cmd + alt - right : yabai -m window --resize right:50:0; \
                  yabai -m window --resize left:50:0
cmd + alt - down : yabai -m window --resize bottom:0:50; \
                  yabai -m window --resize top:0:50
cmd + alt - left : yabai -m window --resize left:-50:0; \
                  yabai -m window --resize right:-50:0

cmd - f : yabai -m window --toggle float;\
          yabai -m window --grid 4:4:1:1:2:2

cmd + ctrl - up : yabai -m window --move rel:0:-20
cmd + ctrl - right : yabai -m window --move rel:20:0
cmd + ctrl - down : yabai -m window --move rel:0:20
cmd + ctrl - left : yabai -m window --move rel:-20:0

cmd + ctrl + alt - up : yabai -m window --resize top:0:-50
cmd + ctrl + alt - right : yabai -m window --resize right:50:0
cmd + ctrl + alt - down : yabai -m window --resize bottom:0:50
cmd + ctrl + alt - left : yabai -m window --resize left:-50:0
